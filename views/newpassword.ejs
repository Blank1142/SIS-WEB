<body class="text-center" style="background-image:url('/image/bg.svg'); background-repeat: no-repeat;
  background-size: auto;">
    <br><br><br>
    <% if(Message) { %>
        <div class="alert alert-danger alert-dismissible fade show" role="alert">
            <%= Message %>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
        <% } %>
            <main class="form-signin">
                <form action="/setpassword?id=<%= empid %>&role=<%= emprole %>" method="post" validation>
                    <img class="mb-4" src="image/nuron 2018-04.png " alt="" width="47" height="47">
                    <h1 class="h3 mb-3 fw-normal font-monospace">Set Password</h1>


    

                    <div class="form-floating" style="display: flex;">
                        <input type="password" class="form-control rounded-pill" id="Password" name="Password"
                          autocomplete="off"  placeholder="name@example.com" required>
                        <span style="  position: absolute;
                          margin-left: 90%;
                          height: 60px;
                          display: flex;
                          align-items: center;">
                            <i class="bi bi-eye-slash" id="togglePassword"></i>
                        
                        </span>
                        <label for="Password"><i class="bi bi-shield-lock"></i> &ensp; New Password</label>
                    </div>

                    <div class="form-floating" style="display: flex;">
                        <input type="password" class="form-control rounded-pill " id="floatingPassword"
                            name="floatingPassword" autocomplete="off" placeholder="Password" required>
                            <span style="  position: absolute;
                              margin-left: 90%;
                              height: 60px;
                              display: flex;
                              align-items: center;">
                                <i class="bi bi-eye-slash" id="togglePasswords"></i>
                            
                            </span>
                        <label for="floatingPassword"><i class="bi bi-shield-lock"></i>&ensp; Re-enter Password</label>
                    </div>

                    <div class="checkbox mb-3">
                       
                    </div>
                    <button class="w-100 btn btn-lg btn-primary rounded-pill" type="submit">Set Password</button>

                </form>
            </main>
<script src="/js/password.js"></script>


            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
