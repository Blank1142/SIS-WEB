
<!--  Toster for chat -->

<div class="m-4">

    <div class="toast" id="myToast" style="position: fixed; 
  margin-top: 70px;
    top: 10px; 
    right: 10px;
">
        <div class="toast-header">
            <i class="bi bi-chat-square-text-fill"> </i> &nbsp; &nbsp;
            <strong class="me-auto" id="tosterSenderName"> </strong>
            <small id="tosterTimeValue"></small>
            <button type="button" class="btn-close" data-bs-dismiss="toast"></button>
        </div>
        <div class="toast-body">
           <p id="tosterMessageValue"></p>
        </div>
    </div>
</div>


<div class="loader-wrapper">
    <span class="loader"><span class="loader-inner"></span></span>
</div>
<script>
    $(window).on("load", function () {
        $(".loader-wrapper").fadeOut("slow");
    });
</script>
